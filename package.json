{
	"name": "@flurrux/insta-loader",
	"version": "1.0.0",
	"description": "download buttons for instagram",
	"scripts": {
		"build:staticfiles": "python static_file_copy.py",
		"build:parcel": "parcel build --no-source-maps --dist-dir ./dist",
		"build": "npm run build:parcel"
	},
	"source": [
		"./app/src/index.ts",
		"./app/src/notifications-background.ts",
		"./app/src/disk-writing/chrome-download-background.ts",
		"./app/manifest.json"
	],
	"author": "Christian Hoffmann <christianhoffmann@outlook.com>",
	"dependencies": {
		"fp-ts": "^2.9.5",
		"ramda": "^0.27.1"
	},
	"browserslist": [
		">5%",
		"Edge > 14",
		"Firefox > 63",
		"Chrome > 67"
	],
	"devDependencies": {
		"@parcel/transformer-raw": "^2.1.1",
		"@types/chrome": "^0.0.129",
		"parcel": "^2.1.0",
		"typescript": "^3.7.5"
	}
}
