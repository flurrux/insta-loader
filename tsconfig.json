{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "react",
		"module": "es2015",
		"moduleResolution": "node",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "./dist",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2018",
		"rootDir": "."
	}
}