{
  "manifest_version": 3,
  "name": "insta-loader",
  "description": "download buttons for instagram media.",
  "version": "1.2.0",
  "icons": {
    "16": "assets/icons/insta-loader-icon-16.png",
    "48": "assets/icons/insta-loader-icon-48.png",
    "128": "assets/icons/insta-loader-icon-128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.index.ts.b3cba241.6b569a9c.js"
      ],
      "matches": [
        "*://www.instagram.com/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "nativeMessaging",
    "tabs",
    "notifications",
    "downloads"
  ],
  "host_permissions": [
    "*://www.instagram.com/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "*://www.instagram.com/*"
      ],
      "resources": [
        "assets/icons/*.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://www.instagram.com/*"
      ],
      "resources": [
        "assets/vendor.8cd73852.js",
        "assets/index.ts.b3cba241.js"
      ],
      "use_dynamic_url": true
    }
  ]
}